{
  "name": "kinto-http",
  "version": "3.0.0",
  "description": "JavaScript HTTP client for the Kinto API.",
  "main": "lib/kinto-http.cjs.js",
  "module": "dist/kinto-http.es.js",
  "jsnext:main": "dist/kinto-http.es.js",
  "scripts": {
    "build:readme": "toctoc -w -d 2 README.md",
    "dist": "npm run dist-es && npm run dist-cjs && npm run dist-dev && npm run dist-noshim && npm run dist-prod && npm run dist-fx",
    "dist-es": "cross-env BABEL_ENV=no-shim rollup -c -e events,uuid/v4,unfetch -o dist/kinto-http.es.js -f es",
    "dist-cjs": "cross-env BABEL_ENV=no-shim rollup -c -e events,uuid/v4,unfetch -o dist/kinto-http.cjs.js -f cjs",
    "dist-dev": "rollup -c -o dist/kinto-http.js",
    "dist-noshim": "cross-env BABEL_ENV=no-shim rollup -c -e events,uuid/v4,unfetch -o dist/kinto-http.noshim.js",
    "dist-prod": "cross-env BABEL_ENV=production rollup -c -o dist/kinto-http.min.js",
    "dist-fx": "cross-env BABEL_ENV=firefox rollup -c -e events -i fx-src/index.js -o temp.jsm && mkdirp dist && cpy --rename moz-kinto-http-client.js fx-src/jsm_prefix.js dist/ && echo \"\n/*\n * Version $npm_package_version - $(git rev-parse --short HEAD)\n */\n\" >> dist/moz-kinto-http-client.js && cat temp.jsm >> dist/moz-kinto-http-client.js && rimraf temp.jsm temp.jsm.map",
    "prepublish": "npm run build:readme",
    "publish-to-npm": "npm run dist && npm publish",
    "report-coverage": "npm run test-cover && coveralls < ./coverage/lcov.info",
    "tdd": "cross-env BABEL_ENV=test mocha --require babel-register --require ./test/setup-jsdom.js --watch 'test/**/*_test.js'",
    "test": "npm run test-nocover",
    "test-cover": "babel-node babel-istanbul cover --report text $npm_package_config_ISTANBUL_OPTS node_modules/.bin/_mocha -- --require ./test/setup-jsdom.js 'test/**/*_test.js'",
    "test-cover-html": "babel-node babel-istanbul cover --report html $npm_package_config_ISTANBUL_OPTS node_modules/.bin/_mocha -- --require ./test/setup-jsdom.js 'test/**/*_test.js' && open coverage/index.html",
    "test-nocover": "babel-node mocha --require ./test/setup-jsdom.js 'test/**/*_test.js'",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kinto/kinto-http.js.git"
  },
  "keywords": [
    "kinto",
    "http",
    "client",
    "api"
  ],
  "author": "Mozilla <storage-team@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Kinto/kinto-http.js/issues"
  },
  "homepage": "https://github.com/Kinto/kinto-http.js#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "unfetch": "^2.1.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "atob": "^2.0.3",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.12.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-babili": "0.0.11",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-3": "^6.22.0",
    "btoa": "^1.1.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.11.16",
    "cpy-cli": "^1.0.1",
    "cross-env": "^3.1.4",
    "esdoc": "^0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-importpath-plugin": "0.1.0",
    "eslint": "3.15.0",
    "form-data": "^2.1.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.10.0",
    "kinto-node-test-server": "0.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "^1.17.7",
    "toctoc": "^0.2.3"
  },
  "engines": {
    "node": ">=6"
  }
}
