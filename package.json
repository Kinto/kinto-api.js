{
  "name": "kinto-http",
  "version": "5.0.0-alpha.2",
  "description": "JavaScript HTTP client for the Kinto API.",
  "main": "lib/cjs-es5/index.js",
  "module": "lib/esm/index.js",
  "unpkg": "dist/kinto-http.min.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:es && npm run build:cjs-es5",
    "build:es": "tsc -p .",
    "build:cjs-es5": "tsc -p . --outDir lib/cjs-es5 --module commonjs --target es5",
    "build:demo": "npm run dist && shx cp dist/kinto-http.min.js demo/kinto-http.min.js && shx cp dist/kinto-http.min.js.map demo/kinto-http.min.js.map",
    "build:readme": "./node_modules/.bin/toctoc -w -d 2 README.md",
    "cs-check": "prettier -l \"{src,test,bin}/**/*.{js,ts}\"",
    "cs-format": "prettier \"{src,test,bin}/**/*.{js,ts}\" --write",
    "demo": "npm run build:demo && http-server demo",
    "dist": "cross-env NODE_ENV=production rollup -c && npm run dist:fx",
    "dist:dev": "rollup -c && npm run dist:fx",
    "dist:fx": "ts-node --skip-project bin/dist-fx.ts",
    "docs": "typedoc",
    "prepublish": "npm run build:readme",
    "publish-to-npm": "npm run build && npm run dist && npm publish",
    "report-coverage": "npm run test-cover && ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "tdd": "mocha -r ts-node/register --require ./test/setup-jsdom.js --watch \"test/**/*_test.{js,ts}\"",
    "test": "npm run test-nocover",
    "test-cover": "nyc --reporter=lcov --reporter=text-summary --report-dir=coverage npm run test-nocover",
    "test-cover-html": "nyc --reporter=html --report-dir=coverage npm run test-nocover && open coverage/index.html",
    "test-nocover": "mocha -r ts-node/register --require source-map-support/register --full-trace --bail --require ./test/setup-jsdom.js \"test/**/*_test.{js,ts}\"",
    "lint": "eslint \"src/**/*.{js,ts}\" \"test/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kinto/kinto-http.js.git"
  },
  "keywords": [
    "kinto",
    "http",
    "client",
    "api"
  ],
  "author": "Mozilla <storage-team@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Kinto/kinto-http.js/issues"
  },
  "homepage": "https://github.com/Kinto/kinto-http.js#readme",
  "dependencies": {
    "uuid": "^7.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-typescript": "^3.0.0",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.1",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.0.13",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "atob": "^2.1.2",
    "btoa": "^1.1.2",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "form-data": "^3.0.0",
    "http-server": "^0.12.0",
    "jsdom": "^16.0.0",
    "kinto-node-test-server": "^1.0.0",
    "mocha": "^7.0.0",
    "node-fetch": "^2.6.0",
    "nyc": "^15.0.0",
    "prettier": "^1.13.0",
    "rollup": "^1.18.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^5.0.0",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "sinon": "^9.0.0",
    "source-map-support": "^0.5.12",
    "toctoc": "^0.3.0",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typedoc": "^0.16.2",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=6"
  }
}
