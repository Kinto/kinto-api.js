{
  "name": "kinto-http",
  "version": "5.0.0-alpha.1",
  "description": "JavaScript HTTP client for the Kinto API.",
  "main": "lib/cjs-es5/index.js",
  "module": "lib/esm/index.js",
  "unpkg": "dist/kinto-http.min.js",
  "scripts": {
    "build": "npm run build:es && npm run build:cjs-es5",
    "build:es": "tsc -p .",
    "build:cjs-es5": "tsc -p . --outDir lib/cjs-es5 --module commonjs --target es5",
    "build:demo": "npm run dist && cp dist/kinto-http.min.js demo/kinto-http.min.js && cp dist/kinto-http.min.js.map demo/kinto-http.min.js.map",
    "build:readme": "./node_modules/.bin/toctoc -w -d 2 README.md",
    "cs-check": "prettier -l '{src,test}/**/*.js'",
    "cs-format": "prettier '{src,test}/**/*.js' --write",
    "demo": "npm run build:demo && http-server demo",
    "dist": "NODE_ENV=production rollup -c && npm run dist:fx",
    "dist:dev": "rollup -c && npm run dist:fx",
    "dist:fx": "mkdir -p dist && cp fx-src/jsm_prefix.js dist/moz-kinto-http-client.js && echo \"\n/*\n * Version $npm_package_version - $(git rev-parse --short HEAD)\n */\n\" >> dist/moz-kinto-http-client.js && cat dist/temp.js >> dist/moz-kinto-http-client.js && rm dist/temp.js && echo >> dist/moz-kinto-http-client.js",
    "prepublish": "npm run build:readme",
    "publish-to-npm": "npm run build && npm run dist && npm publish",
    "report-coverage": "npm run test-cover && ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "tdd": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha -r ts-node/register --require ./test/setup-jsdom.js --watch 'test/**/*_test.{js,ts}'",
    "test": "npm run test-nocover",
    "test-cover": "nyc --reporter=lcov --reporter=text-summary --report-dir=coverage npm run test-nocover",
    "test-cover-html": "nyc --reporter=html --report-dir=coverage npm run test-nocover && open coverage/index.html",
    "test-nocover": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"sourceMap\":true}' mocha -r ts-node/register --require source-map-support/register --full-trace --bail --require ./test/setup-jsdom.js 'test/**/*_test.{js,ts}'",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kinto/kinto-http.js.git"
  },
  "keywords": [
    "kinto",
    "http",
    "client",
    "api"
  ],
  "author": "Mozilla <storage-team@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Kinto/kinto-http.js/issues"
  },
  "homepage": "https://github.com/Kinto/kinto-http.js#readme",
  "dependencies": {
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "atob": "^2.1.2",
    "btoa": "^1.1.2",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.0.0",
    "coveralls": "^3.0.0",
    "esdoc": "^1.1.0",
    "esdoc-accessor-plugin": "^1.0.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-integrate-test-plugin": "^1.0.0",
    "esdoc-publish-html-plugin": "^1.1.2",
    "esdoc-type-inference-plugin": "^1.0.2",
    "esdoc-undocumented-identifier-plugin": "^1.0.0",
    "esdoc-unexported-identifier-plugin": "^1.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "form-data": "^2.1.2",
    "http-server": "^0.11.1",
    "jsdom": "^15.0.0",
    "kinto-node-test-server": "^1.0.0",
    "mocha": "^6.0.0",
    "node-fetch": "^2.6.0",
    "nyc": "^14.1.1",
    "prettier": "^1.13.0",
    "rollup": "^1.18.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sinon": "^7.2.3",
    "source-map-support": "^0.5.12",
    "toctoc": "^0.3.0",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=6"
  }
}
